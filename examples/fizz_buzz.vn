function main() as int
	dim i, LIMIT as int
	LIMIT = 1000000;
	i = 1;
	
	while (i <= LIMIT)
		if (i % 3 == 0) then
			if (i % 5 == 0) then
				println("FizzBuzz");
			else
				println("Fizz");
			end if 
		else
			if (i % 5 == 0) then
				println("Buzz");
			else
				println("${i}");
			end if 
		end if

		++i;
	wend
end function